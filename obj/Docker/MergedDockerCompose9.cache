name: dockercompose9631567809561928932
services:
  mvcmadmagic:
    build:
      context: D:\Projects\MadMagic
      dockerfile: MvcMadMagic/Dockerfile
    depends_on:
      redisapi:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: MadMagicASP
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\SUdez\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\SUdez\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  redis_db:
    image: redis/redis-stack-server
    networks:
      default: null
    ports:
    - mode: ingress
      target: 8001
      published: "8001"
      protocol: tcp
    - mode: ingress
      target: 6379
      published: "6379"
      protocol: tcp
  redisapi:
    build:
      context: D:\Projects\MadMagic
      dockerfile: RedisApi/Dockerfile
    depends_on:
      redis_db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: redisapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: "5000"
      protocol: tcp
    - mode: ingress
      target: 5000
      published: "5001"
      protocol: tcp
networks:
  default:
    name: dockercompose9631567809561928932_default